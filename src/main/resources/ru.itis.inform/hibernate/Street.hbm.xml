<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ru.itis.inform.models.rieltoryModel.Street" table="street" catalog="public">
        <id name="id" type="java.lang.Integer">
            <column name="street_id" />
            <generator class="identity" />
        </id>


        <property name="name" column="street_name"/>




        <many-to-one name="city" column="city_id" class="ru.itis.inform.models.rieltoryModel.City" cascade="all">

        </many-to-one>


    </class>
</hibernate-mapping>