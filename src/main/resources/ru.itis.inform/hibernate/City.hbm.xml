<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ru.itis.inform.models.City" table="city" catalog="public" lazy="false">
        <id name="id" type="java.lang.Integer">
            <column name="city_id" />
            <generator class="identity" />
        </id>


        <property name="name" column="city_name"/>




    </class>
</hibernate-mapping>