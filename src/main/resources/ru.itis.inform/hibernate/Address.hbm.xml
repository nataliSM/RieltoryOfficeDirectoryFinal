<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ru.itis.inform.models.rieltoryModel.Address" table="address" catalog="public">
        <id name="id" type="java.lang.Integer">
            <column name="id_address" />
            <generator class="identity" />
        </id>


        <property name="flat" column="flat"/>
        <property name="house" column="house"/>


        <many-to-one name="city" column="city_id" class="ru.itis.inform.models.rieltoryModel.City" cascade="all">

        </many-to-one>
        <many-to-one name="street" column="street_id" class="ru.itis.inform.models.rieltoryModel.Street" cascade="all">

        </many-to-one>


    </class>
</hibernate-mapping>