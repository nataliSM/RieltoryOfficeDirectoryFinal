<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ru.itis.inform.models.rieltoryModel.Feature" table="features" catalog="public">
        <id name="id" type="java.lang.Integer">
            <column name="features_id" />
            <generator class="identity" />
        </id>


        <property name="countOfRoom" column="count_of_room"/>
        <property name="condition" column="condition"/>
        <property name="repair" column="repair"/>

    </class>
</hibernate-mapping>